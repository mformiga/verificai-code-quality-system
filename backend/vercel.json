{
  "version": 2,
  "name": "verificai-backend",
  "alias": "verificai-backend",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "framework": null,
  "outputDirectory": "",
  "regions": ["iad1"]
}